{
  "name": "bldrs",
  "version": "1.0.0-r383",
  "main": "src/index.jsx",
  "homepage": "https://github.com/bldrs-ai/Share",
  "bugs": {
    "url": "https://github.com/bldrs-ai/Share/issues"
  },
  "scripts": {
    "clean": "shx rm -rf docs ; shx cp -r public docs",
    "new-version": "node src/utils/version.mjs > package.json.new && shx mv package.json.new package.json",
    "copy-web-asm": "shx cp node_modules/web-ifc/*.wasm public/static/js",
    "build": "yarn copy-web-asm && yarn clean && yarn new-version && node config/build.js",
    "serve": "yarn build && node config/serve.js",
    "lint": "yarn eslint src",
    "test": "jest",
    "precommit": "yarn lint && yarn test",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.1.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.1",
    "@mui/styled-engine": "^5.0.1",
    "@mui/styles": "^5.1.0",
    "@octokit/rest": "^18.12.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "three": "^0.135",
    "web-ifc-viewer": "^1.0.184",
    "web-vitals": "^2.1.0",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@alex_neo/jest-expect-message": "^1.0.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-import-assertions": "^7.16.7",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@pablo-mayrgundter/cookies.js": "^1.0.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.9",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.0",
    "babel-jest": "^27.4.4",
    "babel-loader": "^8.2.5",
    "esbuild": "^0.14.22",
    "esbuild-plugin-svgr": "^1.0.0",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jsdom": "^20.0.0",
    "shx": "^0.3.4",
    "storybook-addon-material-ui": "^0.9.0-alpha.24"
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "src/Share.test.js"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "^.+\\.svg$": "<rootDir>/svgTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(web-ifc|web-ifc-three|web-ifc-viewer|three)/)"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js",
      "@alex_neo/jest-expect-message"
    ]
  }
}
