{
  "name": "buildrs",
  "version": "0.1.0-r205",
  "main": "src/index.jsx",
  "homepage": "https://github.com/buildrs/Share",
  "scripts": {
    "clean": "rm -rf docs ; cp -r public docs",
    "new-version": "node --experimental-json-modules src/utils/version.mjs > package.json.new && mv package.json.new package.json",
    "copy-web-asm": "cp node_modules/web-ifc/web-ifc.wasm public/static/js",
    "build": "yarn copy-web-asm && yarn clean && yarn new-version && node config/build.js",
    "serve": "yarn copy-web-asm && node config/serve.js",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.1.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.1",
    "@mui/styled-engine": "^5.0.1",
    "@mui/styles": "^5.1.0",
    "jsonata": "^1.8.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "three": "^0.135",
    "web-ifc-viewer": "^1.0.106",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "babel-jest": "^27.4.4",
    "canvas": "^2.8.0",
    "esbuild": "^0.13.14",
    "gl": "^4.9.2",
    "jest": "^27.4.3",
    "jsdom": "^18.0.1",
    "regenerator-runtime": "^0.13.9"
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "src/App.test.js"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "^.+\\.svg$": "<rootDir>/svgTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(web-ifc|web-ifc-three|web-ifc-viewer|three)/)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  }
}
